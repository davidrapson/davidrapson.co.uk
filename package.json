{
  "name": "davidrapson.co.uk",
  "version": "4.4.0",
  "description": "Personal website",
  "main": "gulpfile.js",
  "repository": "https://github.com/davidrapson/davidrapson.co.uk.git",
  "author": "David Rapson",
  "license": "UNLICENSED",
  "private": true,
  "config": {
    "urls": {
      "dev": "https://davidrapson.site/",
      "prod": "https://davidrapson.co.uk/"
    },
    "aws": {
      "region": "eu-west-1"
    },
    "buildPaths": {
      "sourceDir": "source",
      "sourceDirData": "source/_data",
      "static": "source/assets",
      "styleSrc": "source/assets/stylesheets",
      "styleDest": "source/public/stylesheets",
      "jsSrc": "source/assets/javascripts",
      "jsDest": "source/public/javascripts",
      "publicSource": "source/public",
      "buildDist": "source/public/dist",
      "buildDistCss": "source/public/dist/stylesheets",
      "buildDistJs": "source/public/dist/javascripts",
      "buildDir": "build",
      "publicImages": "build/images",
      "publicDist": "build/public/dist",
      "publicDistCss": "build/public/dist/stylesheets"
    }
  },
  "scripts": {
    "build": "gulp build",
    "deploy": "gulp deploy",
    "test": "npm run build"
  },
  "devDependencies": {
    "aws-sdk": "^2.1.40",
    "babel-core": "^6.2.1",
    "babel-preset-es2015": "^6.1.18",
    "browser-sync": "^2.8.0",
    "del": "^2.0.2",
    "extend": "^3.0.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-concat": "^2.6.0",
    "gulp-csso": "^1.0.0",
    "gulp-gzip": "^1.2.0",
    "gulp-if": "^2.0.0",
    "gulp-imagemin": "^2.3.0",
    "gulp-less": "^3.0.3",
    "gulp-load-plugins": "^1.0.1",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-rev": "^7.0.0",
    "gulp-sass": "^2.0.4",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-util": "^3.0.6",
    "gulp-xo": "^0.7.0",
    "hash-files": "^1.0.0",
    "jshint-stylish": "^2.0.1",
    "mime": "^1.3.4",
    "opn": "^4.0.0",
    "psi": "^2.0.0",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.2",
    "sass-mq": "3.2.6",
    "through2": "^2.0.0"
  },
  "dependencies": {
    "picturefill": "3.0.1"
  },
  "xo": {
    "space": 4
  }
}
